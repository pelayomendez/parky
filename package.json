{
  "name": "Parky",
  "version": "1.0.0",
  "description": "A minimal long term tab retention browser.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package:windows": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/win/icon.ico --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='Parky',
    "package:osx": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/mac/icon.icns --prune=true --out=builds",
    "package:linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/icon.png --prune=true --out=builds"
  },
  "repository": "https://github.com/pelayomendez/parky",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^5.0.0",
    "electron-installer-windows": "^1.1.1",
    "electron-packager": "^13.1.1"
  },
  "dependencies": {
    "dialogs": "^2.0.1",
    "favicon": "^0.0.2",
    "smalltalk": "^3.4.0"
  }
}
